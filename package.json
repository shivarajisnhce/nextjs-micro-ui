{
  "name": "nextjs-micro-ui",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NEXT_PRIVATE_LOCAL_WEBPACK=true next dev",
    "dev:qa": "cross-env NEXT_PRIVATE_LOCAL_WEBPACK=true env-cmd -f .env.qa next dev",
    "build": "cross-env NODE_ENV=production NEXT_PRIVATE_LOCAL_WEBPACK=true next build",
    "build:qa": "cross-env NODE_ENV=production NEXT_PRIVATE_LOCAL_WEBPACK=true env-cmd -f .env.qa next build",
    "start": "next start",
    "start:qa": "env-cmd -f .env.qa next start"
  },
  "dependencies": {
    "concurrently": "^8.2.1",
    "next": "15.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "webpack": "^5.100.2"
  },
  "devDependencies": {
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "typescript": "5.2.2",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0"
  }
}